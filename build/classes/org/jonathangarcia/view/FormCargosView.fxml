<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.jonathangarcia.controller.FormCargosController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/CargoForm.png" />
         </image>
      </ImageView>
      <Button fx:id="btnAceptar" layoutX="8.0" layoutY="184.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="30.0" prefWidth="86.0" text="Aceptar" />
      <Button fx:id="btnCancelar" layoutX="8.0" layoutY="236.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="30.0" prefWidth="86.0" text="Cancelar" />
      <TextArea fx:id="taDescripcion" layoutX="198.0" layoutY="298.0" prefHeight="86.0" prefWidth="284.0" />
      <TextField fx:id="tfCargoId" layoutX="198.0" layoutY="137.0" prefHeight="30.0" prefWidth="214.0" />
      <TextField fx:id="tfNombreCargo" layoutX="198.0" layoutY="206.0" prefHeight="38.0" prefWidth="230.0" />
   </children>
</AnchorPane>
